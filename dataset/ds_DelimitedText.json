			},
			{
				"name": "Nome da rua",
				"type": "String"
			},
			{
				"name": "Endereço completo",
				"type": "String"
			},
			{
				"name": "Estado",
				"type": "String"
			},
			{
				"name": "País",
				"type": "String"
			},
			{
				"name": "Ordem",
				"type": "String"
			}
		]
	}
}