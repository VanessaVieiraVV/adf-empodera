{
	"name": "ds_DelimitedText",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_AzureDataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ds_container": {
				"type": "string"
			},
			"ds_directory": {
				"type": "string"
			},
			"ds_filename": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": "Moovit_Webscraping_RMR.csv",
				"folderPath": "csv",
				"fileSystem": {
					"value": "@dataset().ds_container",
					"type": "Expression"
				}
